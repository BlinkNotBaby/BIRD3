@implementation BIRD3App {
    var sio;
}
-(id)init {
    if(self = [super init]) {
        console.log("Init...");
        sio = io();
        sio.on("connect", function(sock){
            console.log("Connected using:", sio.io.engine.transport.name);
            sio.emit("foo", "bar");
        });
    }
    return self;
}
-(SocketIO)socket {
    return sio;
}
@end

var app = [[BIRD3App alloc] init];
var sio = [app socket];
